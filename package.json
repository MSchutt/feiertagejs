{
  "name": "feiertagejs",
  "version": "1.1.8",
  "description": "A module to calculate German holidays made for node.js",
  "main": "build/feiertage.umd.js",
  "module": "build/feiertage.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --run-in-band",
    "test:w": "jest --watch",
    "format": "prettier --single-quote --write '{**/*,*}.{js,json}' '!coverage/**' '!flow-typed/**'",
    "flow": "flow",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sfakir/feiertagejs"
  },
  "keywords": [
    "node",
    "holidays",
    "feiertage",
    "javascript"
  ],
  "author": "Simon Fakir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfakir/feiertagejs/issues"
  },
  "homepage": "https://github.com/sfakir/feiertagejs",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "documentation": "^5.3.3",
    "eslint": "^4.8.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "flow-bin": "^0.56.0",
    "jest": "20",
    "lodash": "^4.15.0",
    "prettier": "^1.7.4",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%"
            ],
            "node": [
              "4"
            ]
          },
          "modules": false
        }
      ],
      "flow"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ]
  }
}
