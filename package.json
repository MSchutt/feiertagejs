{
  "name": "feiertagejs",
  "version": "1.2.3",
  "description": "A module to calculate German holidays made for node.js",
  "main": "build/feiertage.umd.js",
  "module": "build/feiertage.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --runInBand",
    "test:w": "jest --watch",
    "format": "prettier --write '{**/*,*}.{js,json}' '!coverage/**' '!flow-typed/**' '!build/**' '!package.json'",
    "flow": "flow",
    "build": "rollup -c",
    "build:doc": "documentation build src/**.js -f md -o docs.md --document-exported",
    "build:flow": "flow-copy-source src build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sfakir/feiertagejs"
  },
  "keywords": [
    "node",
    "holidays",
    "feiertage",
    "javascript"
  ],
  "author": "Simon Fakir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfakir/feiertagejs/issues"
  },
  "homepage": "https://github.com/sfakir/feiertagejs",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "documentation": "^5.3.3",
    "eslint": "^4.8.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "flow-bin": "^0.64.0",
    "flow-copy-source": "^1.2.1",
    "jest": "22.1.4",
    "prettier": "^1.14.3",
    "rollup": "^0.55.1",
    "rollup-plugin-babel": "^3.0.7",
    "typescript": "^2.9.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%"
            ],
            "node": [
              "4"
            ]
          },
          "modules": false
        }
      ],
      "flow"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ]
  },
  "files": [
    "build",
    "docs.md",
    "Readme.md",
    "index.d.ts"
  ]
}
